<?xml version="1.0" encoding="UTF-8"?>
<geometricFigures>
    <figure>
        <name>Circle</name>
        <type>2D</type>
        <properties>
            <property>Radius</property>
            <property>Diameter</property>
        </properties>
        <formulas>
            <area>π * radius^2</area>
            <perimeter>2 * π * radius</perimeter>
        </formulas>
    </figure>
    <figure>
        <name>Square</name>
        <type>2D</type>
        <properties>
            <property>Side Length</property>
        </properties>
        <formulas>
            <area>side^2</area>
            <perimeter>4 * side</perimeter>
        </formulas>
    </figure>
	<figure>
		<name>Cube</name>
		<type>3D</type>
		<properties>
			<property>Side Length</property>
			<property>Volume</property>
			<property>Area</property>
		</properties>
		<formulas>
			<Volume>side^3</Volume>
			<Area>6* side^2</Area>
		</formulas>
	</figure>
</geometricFigures>